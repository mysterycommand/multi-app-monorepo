{
  "name": "multi-app-monorepo",
  "version": "1.0.0",
  "lockfileVersion": 2,
  "requires": true,
  "packages": {
    "": {
      "name": "multi-app-monorepo",
      "version": "1.0.0",
      "license": "MIT",
      "workspaces": [
        "apps/*",
        "pkgs/*"
      ]
    },
    "apps/app-a": {
      "name": "@multi-app-monorepo/app-a",
      "version": "1.0.0",
      "bundleDependencies": [
        "@multi-app-monorepo/pkg-c"
      ],
      "license": "ISC",
      "dependencies": {
        "@multi-app-monorepo/pkg-c": "file:../../pkgs/pkg-c"
      }
    },
    "apps/app-b": {
      "name": "@multi-app-monorepo/app-b",
      "version": "1.0.0",
      "license": "ISC"
    },
    "node_modules/@multi-app-monorepo/app-a": {
      "resolved": "apps/app-a",
      "link": true
    },
    "node_modules/@multi-app-monorepo/app-b": {
      "resolved": "apps/app-b",
      "link": true
    },
    "node_modules/@multi-app-monorepo/pkg-c": {
      "resolved": "pkgs/pkg-c",
      "link": true
    },
    "node_modules/@multi-app-monorepo/pkg-d": {
      "resolved": "pkgs/pkg-d",
      "link": true
    },
    "node_modules/@multi-app-monorepo/pkg-e": {
      "resolved": "pkgs/pkg-e",
      "link": true
    },
    "pkgs/pkg-c": {
      "name": "@multi-app-monorepo/pkg-c",
      "version": "1.0.0",
      "license": "ISC"
    },
    "pkgs/pkg-d": {
      "name": "@multi-app-monorepo/pkg-d",
      "version": "1.0.0",
      "license": "ISC"
    },
    "pkgs/pkg-e": {
      "name": "@multi-app-monorepo/pkg-e",
      "version": "1.0.0",
      "license": "ISC"
    }
  },
  "dependencies": {
    "@multi-app-monorepo/app-a": {
      "version": "file:apps/app-a",
      "requires": {
        "@multi-app-monorepo/pkg-c": "file:../../pkgs/pkg-c"
      }
    },
    "@multi-app-monorepo/app-b": {
      "version": "file:apps/app-b"
    },
    "@multi-app-monorepo/pkg-c": {
      "version": "file:pkgs/pkg-c"
    },
    "@multi-app-monorepo/pkg-d": {
      "version": "file:pkgs/pkg-d"
    },
    "@multi-app-monorepo/pkg-e": {
      "version": "file:pkgs/pkg-e"
    }
  }
}
